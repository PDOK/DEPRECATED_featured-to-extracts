<ml:{{_action.delta-type}} objectType="{{_collection}}" objectId="{{_id}}">
    {{#previous}}
    <ml:was id="{{_previous_version}}">
        <mlb:{{featureRootTag}}>
           {{{.}}}
        </mlb:{{featureRootTag}}>
    </ml:was>
    {{/previous}}
    {{#current}}
    <ml:wordt id="{{_version}}">
        <mlb:{{featureRootTag}}>
           {{{.}}}
        </mlb:{{featureRootTag}}>
    </ml:wordt>
    {{/current}}
</ml:{{_action.delta-type}}>